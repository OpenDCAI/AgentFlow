{
  "$schema": "L2 - WebSearch Google API 配置",
  "$resource_type": "websearch",
  "$extends": "default.json",
  
  "description": "Google 搜索 API",
  "backend_class": "sandbox.server.backends.websearch.GoogleSearchBackend",
  
  "config": {
    "search_engine": "google",
    "api_key": "${GOOGLE_API_KEY}",
    "cx": "${GOOGLE_CX}",
    "max_results": 10,
    "timeout": 30
  }
}

