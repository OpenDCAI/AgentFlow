{
  "$schema": "启动配置 - 最小化",
  "$comment": "仅包含 websearch 的最小配置，用于快速测试",

  "server": {
    "title": "Sandbox Minimal",
    "session_ttl": 300,
    "log_level": "INFO"
  },

  "resources": {
    "websearch": {
      "enabled": true,
      "description": "Web 搜索（Mock）",
      "backend_class": "sandbox.server.backends.resources.websearch.WebSearchBackend",
      "config": {
        "search_engine": "mock",
        "max_results": 10
      }
    }
  }
}

